<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mall.coupon.mapper.SeckillSessionMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.mall.coupon.entity.SeckillSessionEntity" id="seckillSessionMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="startTime" column="start_time"/>
        <result property="endTime" column="end_time"/>
        <result property="status" column="status"/>
        <result property="createTime" column="create_time"/>
    </resultMap>
    <select id="querySessionByDays" resultType="com.mall.coupon.entity.SeckillSessionEntity">
        select *
        from mall_sms.sms_seckill_session
        where start_time between #{beginDay} and #{endDay}
    </select>


</mapper>